{
  "name": "data-typed",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/data-typed.umd.js",
  "module": "dist/data-typed.es5.js",
  "typings": "dist/types/data-typed.d.ts",
  "files": [
    "dist"
  ],
  "author": "Pranjal Raihan <pranjal@pranjal-raihan.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --format codeFrame --project . 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs --outDir dist/lib && rollup -c rollup.config.ts && typedoc --out dist/docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:prod": "npm run lint && npm run test:coverage",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.0.0",
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.0",
    "lodash": "^4.17.4",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.4.4",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.1",
    "rollup": "^0.53.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.9.0",
    "source-map-support": "^0.5.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-no-unused-expression-chai": "0.0.3",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "validate-commit-msg": "^2.12.2"
  },
  "peerDependencies": {
    "reflect-metadata": "*"
  },
  "dependencies": {}
}
